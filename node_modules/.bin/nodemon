#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/nodemon@2.0.7/node_modules/nodemon/bin/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/nodemon@2.0.7/node_modules/nodemon/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/nodemon@2.0.7/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules:/home/workspace/.tmp/nodejs/coding-practices/node_modules:/home/workspace/.tmp/nodejs/node_modules:/home/workspace/.tmp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/nodemon/bin/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/nodemon/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/nodemon@2.0.7/node_modules/nodemon/bin/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/nodemon@2.0.7/node_modules/nodemon/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/nodemon@2.0.7/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules:/home/workspace/.tmp/nodejs/coding-practices/node_modules:/home/workspace/.tmp/nodejs/node_modules:/home/workspace/.tmp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/nodemon/bin/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/nodemon/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
else
  exec node  "$basedir/../nodemon/bin/nodemon.js" "$@"
fi
