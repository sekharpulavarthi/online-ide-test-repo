#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/mkdirp@0.5.6/node_modules/mkdirp/bin/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/mkdirp@0.5.6/node_modules/mkdirp/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/mkdirp@0.5.6/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules:/home/workspace/.tmp/nodejs/coding-practices/node_modules:/home/workspace/.tmp/nodejs/node_modules:/home/workspace/.tmp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/tar@4.4.19/node_modules/tar/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/tar@4.4.19/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/mkdirp@0.5.6/node_modules/mkdirp/bin/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/mkdirp@0.5.6/node_modules/mkdirp/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/mkdirp@0.5.6/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules:/home/workspace/.tmp/nodejs/coding-practices/node_modules:/home/workspace/.tmp/nodejs/node_modules:/home/workspace/.tmp/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/tar@4.4.19/node_modules/tar/node_modules:/home/workspace/.tmp/nodejs/coding-practices/coding-practice-4a/node_modules/.pnpm/tar@4.4.19/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mkdirp@0.5.6/node_modules/mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../../../../../mkdirp@0.5.6/node_modules/mkdirp/bin/cmd.js" "$@"
fi
